#include<stdio.h>
void main()
{
    int i,j,n,temp;
    char a[100],b[100];
    printf("Enter A string\n");
    gets(a);
    while(a[n]!='\0')
    {
        n++;
    }
    for(i=0;i<=(n-1)/2;i++)
    {
        temp=a[n-i-1];
        a[n-i-1]=a[i];
        a[i]=temp;
    }
    printf("The rev str is %s",a);
}
